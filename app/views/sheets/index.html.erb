<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>Time In</th>
      <th>Time Out</th>
      <th>Lunch Start</th>
      <th>Lunch End</th>
      <th>Duration</th>
      <th>Net Pay</th>
    </tr>
  </thead>
  <tbody>
    <% @sheets.each do |sheet| %>
      <tr>
        <td><%= sheet.get_date %></td>
        <td><%= sheet.get_time(:in_time) %></td>
        <td><%= sheet.get_time(:out_time) %></td>
        <td><%= sheet.get_time(:lunch_start) %></td>
        <td><%= sheet.get_time(:lunch_end) %></td>
        <td><%= sheet.get_duration %></td>
        <td><%= sheet.get_pay %></td>
      </tr>
    <% end %>
  </tbody>
</table>